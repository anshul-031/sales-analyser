  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build && npx prisma db push",
    "build:watch": "nodemon --watch src --ext ts,tsx --exec \"npm run build && npm run dev\"",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
